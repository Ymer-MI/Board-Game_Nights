{
  "kind": "collectionType",
  "collectionName": "clients",
  "info": {
    "singularName": "client",
    "pluralName": "clients",
    "displayName": "Clients"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "private": true,
      "required": true,
      "minLength": 6,
      "maxLength": 128,
      "unique": true
    },
    "token": {
      "type": "string",
      "minLength": 128,
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "maxLength": 64,
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::event.event",
      "mappedBy": "host"
    }
  }
}
